{
    "name": "cosmo-microservice/service-response",
    "description": "Response service for Symfony & Laravel/Lumen Framework.",
    "keywords": ["Symfony", "laravel", "lumen", "rest", "http", "response"],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Fabrizio Cafolla",
            "email": "developer@fabriziocafolla.com",
            "homepage": "https://fabriziocafolla.com",
            "role": "Developer"
        }
    ],

    "support": {
        "email": "developer@fabriziocafolla.com",
        "wiki": "https://github.com/FabrizioCafolla/response-http/wiki"
    },

    "require": {
        "php": ">=7.1.3",
        "symfony/http-kernel": "^4.2"
    },

    "replace": {
        "ServiceResponse/laravel": "self.version",
        "ServiceResponse/response": "self.version"
    },

    "autoload": {
        "psr-4": {
            "ServiceResponse\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
