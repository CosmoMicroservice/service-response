{
    "name": "fabrizio-cafolla/response-http",
    "description": "Response service for Laravel/Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen", "graphql", "rest", "http", "response"],
    "license": "MIT",
    "type": "lumen-package",
    "authors": [
        {
            "name": "Fabrizio Cafolla",
            "email": "info@fabriziocafolla.com"
        }
    ],

    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": ">=5.7"
    },

    "autoload": {
        "psr-4": {
            "ResponseHTTP\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ResponseHTTP\\HttpServiceProvider"
            ]
        }
    }
}
